<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>todoist-tracker-weekly-update</string>
        <key>Program</key>
        <string>/Users/<USER>/Scripts/todoist-tracker-weekly-goal-update.sh</string>
        <key>StartCalendarInterval</key>
        <dict>
            <key>Weekday</key>
            <integer>1</integer>
            <key>Hour</key>
            <integer>1</integer>
            <key>Minute</key>
            <integer>0</integer>
        </dict>
        <key>StandardErrorPath</key>
        <string>/tmp/todoist-tracker.err</string>

        <key>StandardOutPath</key>
        <string>/tmp/todoist-tracker.out</string>
    </dict>
</plist>